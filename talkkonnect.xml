<?xml version="1.0" encoding="UTF-8"?>
<document type="talkkonnect/xml">
	<accounts>
		<account name="talkkonnect-go" default="true">
			<serverandport>127.0.0.1:64738</serverandport>
			<username></username>
			<password>talkkonnect</password>
			<insecure>true</insecure>
			<certificate></certificate>
			<channel></channel>
			<ident>Name Surname</ident>
		</account>
	</accounts>
	<global>
		<software>
			<settings>
				<outputdevice>Headphone Playback ZC</outputdevice>
				<outputdeviceshort>Headphone</outputdeviceshort>
				<logfilenameandpath>/var/log/talkkonnect.log</logfilenameandpath>
				<logging>screenandfilewithlineno</logging>
				<loglevel>debug</loglevel>
				<daemonize>false</daemonize>
				<cancellablestream>true</cancellablestream>
				<simplexwithmute>false</simplexwithmute>
				<txcounter>false</txcounter>
				<nextserverindex>0</nextserverindex>
			</settings>
			<autoprovisioning enabled="false">
				<tkid></tkid>
				<url>http://mycustomdomain.com</url>
				<savefilepath>~/go/src/github.com/jdiderik/talkkonnect</savefilepath>
				<savefilename>talkkonnect.xml</savefilename>
			</autoprovisioning>
			<sounds>
				<event enabled="true">
					<joinedfilenameandpath>~/go/src/github.com/jdiderik/talkkonnect/soundfiles/events/event.wav</joinedfilenameandpath>
					<leftfilenameandpath>~/go/src/github.com/jdiderik/talkkonnect/soundfiles/events/left.wav</leftfilenameandpath>
					<messagefilenameandpath>~/go/src/github.com/jdiderik/talkkonnect/soundfiles/events/message.wav</messagefilenameandpath>
				</event>
				<alert enabled="true">
					<filenameandpath>~/go/src/github.com/jdiderik/talkkonnect/soundfiles/alerts/alert.wav</filenameandpath>
					<volume>1</volume>
				</alert>
				<incommingbeep enabled="false">
					<filenameandpath>~/go/src/github.com/jdiderik/talkkonnect/soundfiles/rogerbeeps/Incoming.wav</filenameandpath>
					<volume>1</volume>
				</incommingbeep>
				<rogerbeep enabled="true">
					<filenameandpath>~/go/src/github.com/jdiderik/talkkonnect/soundfiles/rogerbeeps/Image.wav</filenameandpath>
					<volume>1</volume>
				</rogerbeep>
				<repeatertone enabled="true">
		                	<tonefrequencyhz>1750</tonefrequencyhz>
                			<tonedurationsec>1</tonedurationsec>
				</repeatertone>
				<stream enabled="true">
					<filenameandpath>http://mycustomdomain.com:8200</filenameandpath>
					<volume>0.5</volume>
				</stream>
			</sounds>
			<txtimeout enabled="false">
				<txtimeoutsecs>60</txtimeoutsecs>
			</txtimeout>
			<api enabled="true">
				<apilistenport>8080</apilistenport>
				<displaymenu>true</displaymenu>
				<channelup>true</channelup>
				<channeldown>true</channeldown>
				<mute>true</mute>
				<currentvolumelevel>true</currentvolumelevel>
				<digitalvolumeup>true</digitalvolumeup>
				<digitalvolumedown>true</digitalvolumedown>
				<listserverchannels>true</listserverchannels>
				<starttransmitting>true</starttransmitting>
				<stoptransmitting>true</stoptransmitting>
				<listonlineusers>true</listonlineusers>
				<playstream>true</playstream>
				<requestgpsposition>true</requestgpsposition>
				<previousserver>false</previousserver>
				<nextserver>true</nextserver>
				<panicsimulation>true</panicsimulation>
				<scanchannels>true</scanchannels>
				<displayversion>true</displayversion>
				<clearscreen>true</clearscreen>
				<repeattxlooptest>true</repeattxlooptest>
				<printxmlconfig>true</printxmlconfig>
				<sendemail>true</sendemail>
				<pingservers>true</pingservers>
			</api>
			<mqtt enabled="false">
				<mqtttopic>thailand/bangkok/company/talkkonnect</mqtttopic>
				<mqttbroker>tcp://mqtt.test.com:1883</mqttbroker>
				<mqttpassword>test</mqttpassword>
				<mqttuser>test</mqttuser>
				<mqttid>talkkonnect</mqttid>
				<cleansess>false</cleansess>
				<qos>0</qos>
				<num>1</num>
				<payload></payload>
				<action>sub</action>
				<store></store>
			</mqtt>
		</software>
		<hardware targetboard="rpi">
		</hardware>
	</global>
</document>

